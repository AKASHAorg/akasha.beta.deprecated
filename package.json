  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  },
  "engines": {
    "electron": "1.4.15"
  },
  "build": {
    "appId": "org.develar.ElectronReact",
    "category": "public.app-category.network",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "main/",
      "!main/*.ts",
      "!main/**/*.ts",
      "!main/test/*",
      "!main/test/**/*",
      "!main/typings/*",
      "!main/typings/**/*",
      "!test${/*}*",
      "!app/**/*",
      "!intl/*",
      "!intl/**/*",
      "node_modules/",
      "app/app.html",
      "main.js",
      "main.js.map",
      "LICENSE",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "zip"
      ]
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": true
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  }
}
