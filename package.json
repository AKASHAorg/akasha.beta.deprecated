  "build": {
    "appId": "com.electron.akasha",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ],
      "icon": "icon.icns"
    },
    "files": [
      "dist",
      "!main/",
      "!main/**/*",
      "!**/node_modules/babel-*",
      "!test${/*}*",
      "!app/**/*",
      "!app/",
      "LICENSE",
      "package.json"
    ],
    "publish": [
      "github"
    ],
    "win": {
      "certificateSubjectName": "AKASHA International GmbH",
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "icon.ico"
    },
    "linux": {
      "category": "Network",
      "target": [
        "deb",
        "zip",
        "AppImage",
        "snap"
      ],
      "icon": "icons"
    },
    "snap": {
      "grade": "stable"
    },
    "mac": {
      "category": "public.app-category.social-networking",
      "target": [
        "dmg",
        "zip",
        "pkg"
      ],
      "icon": "icon.icns"
    },
    "nsis": {
      "oneClick": false,
      "multiLanguageInstaller": true,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  }
}
