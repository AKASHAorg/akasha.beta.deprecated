{
  "name": "akasha_platform",
  "productName": "AKASHA",
  "private": true,
  "description": "Akasha platform",
  "main": "main/index.js",
  "scripts": {
    "test": "better-npm-run test",
    "test-watch": "npm test -- --watch",
    "debug-ipc": "ELECTRON_RUN_AS_NODE=true node_modules/.bin/electron node_modules/node-inspector/bin/inspector.js --no-preload --web-port 8181",
    "test-ipc": "node_modules/.bin/mocha test/spectron/spectron-main.js",
    "test-real": "node_modules/.bin/mocha test/spectron/spectron-real.js",
    "lint": "node node_modules/eslint/bin/eslint.js app/ electron-api/",
    "browser": "node server.js",
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "wingui": "better-npm-run wingui",
    "xgui": "better-npm-run xgui",
    "package": "better-npm-run package",
    "package-all": "npm run package -- --all"
  },
  "betterScripts": {
    "start": {
      "command": "electron ./",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "wingui": {
      "command": "node_modules\\.bin\\electron.cmd --remote-debugging-port=1337 .",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "xgui": {
      "command": "node_modules/.bin/electron --remote-debugging-port=1337 --debug=5858 .",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "package": {
      "command": "node package.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "./node_modules/.bin/mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AkashaProject/node-app.git"
  },
  "author": {
    "name": "Akasha Team",
    "email": "hi@akasha.world",
    "url": "https://github.com/AkashaProject"
  },
  "license": "MPL 2.0",
  "bugs": {
    "url": "https://github.com/AkashaProject/node-app/issues"
  },
  "homepage": "https://github.com/AkashaProject/node-app#readme",
