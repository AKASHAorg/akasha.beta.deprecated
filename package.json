    "devEngines": {
        "node": ">=6.x",
        "npm": ">=3.x"
    },
    "engines": {
        "electron": "1.7.8"
    },
    "build": {
        "appId": "com.electron.akasha",
        "dmg": {
            "contents": [
                {
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "files": [
            "dist/",
            "main/",
            "!main/*.ts",
            "!main/**/*.ts",
            "!main/test/*",
            "!main/test/**/*",
            "!main/typings/*",
            "!main/typings/**/*",
            "!test${/*}*",
            "!app/**/*",
            "!intl/*",
            "!intl/**/*",
            "node_modules/",
            "main.js",
            "main.js.map",
            "LICENSE",
            "package.json"
        ],
        "win": {
            "certificateSubjectName": "AKASHA International GmbH",
            "target": [
                "nsis",
                "zip"
            ]
        },
        "linux": {
            "category": "Network",
            "target": [
                "deb",
                "zip"
            ]
        },
        "mac": {
            "category": "public.app-category.social-networking",
            "target": [
                "dmg",
                "zip"
            ]
        },
        "nsis": {
            "oneClick": false,
            "multiLanguageInstaller": true,
            "allowToChangeInstallationDirectory": true
        },
        "directories": {
            "buildResources": "resources",
            "output": "release"
        }
    }
}
