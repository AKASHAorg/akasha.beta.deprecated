{
  "name": "akasha_platform",
  "productName": "AKASHA",
  "private": true,
  "description": "Akasha platform",
  "main": "main.js",
  "scripts": {
    "test": "better-npm-run test",
    "test-watch": "npm test -- --watch",
    "test-e2e": "better-npm-run test-e2e",
    "lint": "node node_modules/eslint/bin/eslint.js app/ electron-api/",
    "browser": "node server.js",
    "build": "better-npm-run build",
    "coverage": "better-npm-run coverage",
    "start": "better-npm-run start",
    "wingui": "better-npm-run wingui",
    "xgui": "better-npm-run xgui",
    "package": "better-npm-run package",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json&& cd node_modules/leveldown && node-gyp rebuild --target=0.37.8 --arch=x64 --dist-url=https://atom.io/download/atom-shell"
  },
  "betterScripts": {
    "start": {
      "command": "electron ./",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "wingui": {
      "command": "node_modules\\.bin\\electron.cmd --remote-debugging-port=1337 main.js",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "xgui": {
      "command": "node_modules/.bin/electron --remote-debugging-port=1337 main.js",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "package": {
      "command": "node package.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-e2e": {
      "command": "mocha --compilers js:babel-core/register --require ./test/setup.js --require co-mocha ./test/e2e.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "coverage": {
      "command": "istanbul cover _mocha -- -R spec",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AkashaProject/node-app.git"
  },
  "author": {
    "name": "Akasha Team",
    "email": "hi@akasha.world",
    "url": "https://github.com/AkashaProject"
  },
  "license": "MPL 2.0",
  "bugs": {
    "url": "https://github.com/AkashaProject/node-app/issues"
  },
  "homepage": "https://github.com/AkashaProject/node-app#readme",
