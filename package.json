{
  "name": "AKASHA",
  "productName": "AKASHA",
  "private": true,
  "description": "Akasha platform",
  "main": "desktop/index.js",
  "version": "0.7.2",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --require test/test-setup.js --compilers js:babel-register test/app/setup.js --recursive test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "debug-ipc": "cross-env ELECTRON_RUN_AS_NODE=true node_modules/.bin/electron node_modules/node-inspector/bin/inspector.js --no-preload --web-port 8181",
    "lint": "eslint app *.js",
    "browser": "node server.js",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=4096 -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "hot-server-dark": "cross-env NODE_ENV=development DARK_THEME=1 node --max_old_space_size=4096  -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max-old-space-size=5096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build-vendor": "cross-env NODE_ENV=production node --max-old-space-size=5096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.vendor.js --progress --profile --colors",
    "build-renderer-dark": "cross-env NODE_ENV=production DARK_THEME=1 node --max-old-space-size=5096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "tsc -p tsconfig.json && npm run build-renderer && npm run build-renderer-dark",
    "build-lang": "babel ./app/utils/build-translations.js | node",
    "flow": "./node_modules/.bin/flow",
    "flow-typed": "flow-typed install",
    "lang-diff": "babel ./app/utils/create-lang-diff.js | node",
    "start": "cross-env NODE_ENV=production electron ./dist/main/index.js",
    "prestart-hot": "npm run build-lang",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron --inspect .",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "package": "npm run build && build --publish never",
    "package-win": "cross-env DEBUG=electron-builder,electron-builder:* --win --x64",
    "package-linux": "cross-env DEBUG=electron-builder,electron-builder:* --linux",
    "package-mac": "cross-env DEBUG=electron-builder,electron-builder:* --mac",
    "package-all": "cross-env DEBUG=electron-builder,electron-builder:* -mwl",
    "release": "npm run build && cross-env DEBUG=electron-builder,electron-builder:* electron-builder",
    "generate-changelog": "standard-version",
    "postinstall": "npm run flow-typed"
  },
  "betterScripts": {
    "start": {
      "command": "cross-env electron ./",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-hot": {
      "command": "cross-env HOT=1 electron -r babel-register -r babel-polyfill ./main",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "package": {
      "command": "node package.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "cross-env BABEL_DISABLE_CACHE=1 mocha --compilers js:babel-register --recursive test/**/*.spec.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AkashaProject/dapp"
  },
  "author": {
    "name": "AKASHA International GmbH",
