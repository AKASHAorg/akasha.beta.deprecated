{
  "name": "akasha_platform",
  "productName": "AKASHA",
  "private": true,
  "description": "Akasha platform developed with electron and react",
  "main": "main.js",
  "scripts": {
    "test": "better-npm-run test",
    "test-watch": "npm test -- --watch",
    "test-e2e": "better-npm-run test-e2e",
    "lint": "node node_modules/eslint/bin/eslint.js app/ electron-api/",
    "hot-server": "node server.js",
    "build": "better-npm-run build",
    "coverage": "better-npm-run coverage",
    "start": "better-npm-run start",
    "start-hot": "better-npm-run start-hot",
    "package": "better-npm-run package",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json && cd node_modules/leveldown && HOME=~/.electron-gyp node-gyp rebuild --target=0.36.7 --arch=x64 --dist-url=https://atom.io/download/atom-shell"
  },
  "betterScripts": {
    "start": {
      "command": "electron ./",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-hot": {
      "command": "electron --remote-debugging-port=1337 ./",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "package": {
      "command": "node package.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-e2e": {
      "command": "mocha --compilers js:babel-core/register --require ./test/setup.js --require co-mocha ./test/e2e.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "coverage": {
      "command": "istanbul cover _mocha -- -R spec",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron",
    "ipfs": "./node_modules/.bin/ipfs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AkashaProject/node-app.git"
  },
  "author": {
    "name": "Akasha Team",
    "email": "hi@akasha.world",
    "url": "https://github.com/AkashaProject"
  },
  "license": "MPL 2.0",
  "bugs": {
    "url": "https://github.com/AkashaProject/node-app/issues"
  },
  "homepage": "https://github.com/AkashaProject/node-app#readme",
  "devDependencies": {
    "asar": "^0.10.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "co-mocha": "^1.1.2",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "cssnano": "^3.5.2",
    "del": "^2.2.0",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "^0.37.3",
    "electron-rebuild": "^1.1.3",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.5.0",
    "file-loader": "^0.8.5",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
